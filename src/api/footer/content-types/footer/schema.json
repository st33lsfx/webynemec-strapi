    "menuItems": {
      "type": "component",
      "repeatable": true,
      "component": "shared.link"
    },
    "servicesItems": {
      "type": "component",
      "repeatable": true,
      "component": "shared.link"
    },
    "contactEmail": {
      "type": "string",
      "default": "info@webynemec.cz"
    },
    "contactPhone": {
      "type": "string",
      "default": "+420 123 456 789"
    },
    "contactAddress": {
      "type": "string",
      "default": "Praha, Česká republika"
    },
    "socialLinks": {
      "type": "component",
      "repeatable": true,
      "component": "footer.social-link"
    },
    "copyrightText": {
      "type": "string",
      "default": "Ondřej Němec. Všechna práva vyhrazena."
    },
